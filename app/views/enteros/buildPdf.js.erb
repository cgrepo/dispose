var $rows = $('.tbone').find('tr');
var enteros = [];

$rows.each(
  function(i,el){
    var $col = $(this).find('td');
    enteros.push($col.eq(0).text());
  });

var url = '/enteros/showEntero?totals=' + enteros;

$('#myIframe').attr("src", url);
$('.tag').show();
$('.pdfcol').show();

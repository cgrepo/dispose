var id = $('.display').find('tr:last td:first').text();

$.ajax({
    type:'DELETE',
    url:'/enteros/popService',
    dataType:'json',
    data: { id: id }
    ,success: function(data)
    { 
        $('.display tr:last').remove();
        //console.log('del ' + $('.tbone').children('tr').length);
        var rowCount = $('.tbone').children('tr').length;
        if (rowCount == 0)
        {
            $('.btnrm').hide();
            $('.generate').hide();
            $('.tag').hide();
        }
        
        if( rowCount <= 6 && rowCount >= 1)
        {
            $('.btnplus').show();
            $('.generate').show();
        }
    }
    ,error:function(data)
    { 
        console.log(data); debugger; 
    }
});


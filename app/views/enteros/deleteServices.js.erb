var id = $('.display').find('tr:last td:first').text();
var serviceID = $('.tbone').find('tr:last td:eq(1)').text();
console.log(serviceID);
$.ajax({
    type:'DELETE',
    url:'/enteros/popService',
    dataType:'json',
    data: { id: id }
    ,success: function(data)
    {
        $('.services option[value="'+serviceID+'"]').removeAttr('disabled');
        $('.display tr:last').remove();
        var rowCount = $('.tbone').children('tr').length;
        if (rowCount == 0)
        {
            $('.btnrm').hide();
            $('.generate').hide();
            $('.tag').hide();
            $('#taxpayer').prop('disabled',false);
        }
        
        if( rowCount <= 6 && rowCount >= 1)
        {
            $('.btnplus').show();
            $('.generate').show();
        }
    }
    ,error:function(data)
    { 
        console.log(data); debugger; 
    }
});

